      <m v="city">
        <m zoom-min="7">
          <caption k="name" style="bold" size="19" fill="#000000" stroke="#ffffff" stroke-width="2.0" priority="1"
            dy="14" symbol="city" />
        </m>
        <m zoom-max="6">
          <caption k="name" size="19" fill="#000000" stroke="#ffffff" stroke-width="2.0" priority="1" dy="14"
            symbol="city" />
